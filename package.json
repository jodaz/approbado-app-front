{
  "name": "Approbado",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run start:api\" \"npm run start:ui\"",
    "start:api": "npm run watch:dev --prefix api",
    "start:ui": "npm run start --prefix ui",
    "migrate": "npm run db:migrate --prefix api",
    "seed": "npm run seed:run --prefix api",
    "rollback": "npm run db:rollback --prefix api"
  }
}
