{
  "name": "Approbado",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "scripts": {
    "start:all": "npm-run-all start:api start:ui",
    "start:api": "npm run watch:dev --prefix api",
    "start:ui": "npm run start --prefix ui",
    "migrate": "npm run @approbado/api run db:migrate",
    "seed": "npm run @approbado/api run seed:run",
    "rollback": "npm run @approbado/api run db:rollback"
  }
}
